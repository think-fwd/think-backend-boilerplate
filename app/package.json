{
  "name": "thinkcrm-server",
  "version": "1.0.0",
  "description": "monolytic api to handle thinkcrm app requests",
  "author": "Think Forward",
  "license": "ISC",
  "main": "server.js",
  "scripts": {
    "kill": "kill -9 $(lsof -ti:1337)",
    "serverless:build": "serverless-build-modules --file ../serverless/serverless.yml --dev",
    "script": "tsx ./scripts/index.ts",
    "develop": "tsx watch src/presenter/http/express/develop.ts",
    "start": "node ./build/src/presenter/http/express/server.js",
    "build": "tsc",
    "prebuild": "rimraf build",
    "postbuild": "copyfiles package.json build && copyfiles -u 1 src/**/*.prisma build/src & copyfiles -u 1 src/**/*.html build/src",
    "postinstall": "npm run prisma:generate",
    "lint": "eslint ./src/",
    "test": "LOG_DISABLED=true NODE_OPTIONS='--experimental-vm-modules --no-warnings --experimental-json-modules' jest --updateSnapshot --no-cache --verbose",
    "test:coverage": "LOG_DISABLED=true NODE_OPTIONS='--experimental-vm-modules --no-warnings --experimental-json-modules' jest --updateSnapshot --no-cache --coverage tests/unit",
    "prisma:generate": "prisma generate",
    "setup:mongodb": "tsx ./setup/mongodb/index.ts",
    "setup:migrations": "tsx ./setup/migrations/index.ts"
  },
  "devDependencies": {
    "@types/express-fileupload": "^1.4.1",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.184",
    "@types/mime-types": "^2.1.1",
    "@types/shelljs": "^0.8.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "prisma": "^5.3.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^11.2.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-mockery": "^1.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ttypescript": "^1.5.13",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.292.0",
    "@aws-sdk/client-ses": "^3.303.0",
    "@aws-sdk/lib-storage": "^3.294.0",
    "@aws-sdk/s3-presigned-post": "^3.292.0",
    "@aws-sdk/s3-request-presigner": "^3.421.0",
    "@prisma/client": "^5.3.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "deepPick": "^1.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "google-libphonenumber": "^3.2.30",
    "indicative": "^7.4.4",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.44",
    "mongodb": "^6.1.0",
    "prompts": "^2.4.2",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "serverless-http": "^3.1.0",
    "serverless-plugin-typescript": "^2.1.4",
    "shelljs": "^0.8.5",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0",
    "validator-brazil": "^1.2.2"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=10.0.0"
  }
}
